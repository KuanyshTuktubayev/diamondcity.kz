<div id="loginblock" style="display: none;">
    <div class="container" id="cab">
        <p>Вход в личный кабинет клиента</p>
        <form action="/cab" method="post" name="CabLoginForm">
            <input type="text" name="login" id="login" placeholder="ID" />
            <input type="password" name="pwd" id="pwd" placeholder="пароль" />
            <div id="divRegNewClient" style="display: none;">
				<div class="form-group" id="divFIO" hidden>
					<input type="text" name="Lastname" id="Lastname" placeholder="Фамилия"  />
					<input type="text" name="Firstname" id="Firstname" placeholder="Имя"  />
					<input type="text" name="Middlename" id="Middlename" placeholder="Отчество"  />
				</div>
				<div class="form-group" id="divIIN" hidden>
					<input type="text" name="IIN" id="IIN" placeholder="ИИН" hidden />
					<input type="email" name="email" id="email" placeholder="email" hidden />
				</div>
				<div class="form-group" id="divMobphone" hidden>
					<label for="Mobilephone"id="lbMobphone" hidden>Моб. тел. (7-7ХХ-ХХ-ХХ)</label>
					<input type="tel" name="Mobilephone" id="Mobilephone" pattern="7-7[0-9]{2}-[0-9]{3}-[0-9]{2}-[0-9]{2}" value="7-700-000-00-00" hidden />
				</div>
				<div class="form-group" id="divIDParent" hidden>
					<input type="text" name="IDParent" id="IDParent" placeholder="ID пригласившего" hidden />
				</div>
				<div class="form-group" id="divPlantype" hidden>
					<label for="selIDPlanType">Вид входа</label>
					<select id="selIDPlanType" name="IDPlanType">
						<option value="1" selected>20'000-Start</option>
						<option value="2">50'000-Сапфир</option>
						<option value="4">200'000-VIP</option>
					</select>
				</div>
			</div>
            <input type="submit" class="btn btn-primary" name="btnLogin" name="btnLogin" value="Войти" />
			<button class="btn btn-link" formaction="/forgetpwd" id="linkForgetPwd">Забыли пароль?</button>
            <br />
			<div id="divMSGForgetPwd"></div>
            <input type="checkbox" name="registration" id="registration" /> Регистрация
        </form>
        <script>
            $('#registration').click(function() {
                if ($("#registration").is(":checked")) {
                    $("#divRegNewClient").show();
					
					$('#login').hide(1000);
					//$('#Lastname').show(1000);
                    //$('#Firstname').show(1000);
					//$('#Middlename').show(1000);
                    $('#IDParent').show(1000);
					$('#email').show(1000);
					$('#IIN').show(1000);
					$('#lbMobphone').show(1000);
					$('#Mobilephone').show(1000);
					$('#divFIO').show(1000);
					$('#divIIN').show(1000);
					$('#divMobphone').show(1000);
					$('#divIDParent').show(1000);
					$('#divPlantype').show(1000);
                } else {
					$("#divRegNewClient").hide();
					
					$('#login').show(1000);
                    //$('#Lastname').hide(1000);
                    //$('#Firstname').hide(1000);
					//$('#Middlename').hide(1000);
                    $('#IDParent').hide(1000);
					$('#email').hide(1000);
					$('#IIN').hide(1000);
					$('#lbMobphone').hide(1000);
					$('#Mobilephone').hide(1000);
					$('#divFIO').hide(1000);
					$('#divIIN').hide(1000);
					$('#divMobphone').hide(1000);
					$('#divIDParent').hide(1000);
					$('#divPlantype').hide(1000);
                }
            });
			$("#linkForgetPwd").click(function() {
				console.log("forgetPWD clicked");
