<script>
	$(document).ready(function(){
		$.ajax({
			url: '/productprices',
			method: "GET",
			data: {},
			dataType: "json",
			cache: false,
			success: function (data) {
				var tbl = '<ul>'+
						'<li class="wow fadeInRightBig" style="background-color: #2c3293;">'+
                            '<div class="_price_table_left"><strong>Название товара</strong></div>'+
                            '<div class="_price_table_right"><strong><span>Цена</span></strong></div>'+
                        '</li>';
				var tr = '';
				
				var divPrList = '';
				var divPrDescr = '';
				
				$.each(data, function (i, attr) {
					$.each(attr, function (j, val) {
						if ((val.isShowPrice == 1) && (val.price > 0)) {
							tr = '<li class="wow fadeInRightBig">' +
									'<div class="_price_table_left"><p>' +
									'<a href="#" data-target="#product-'+val.ID+'" data-toggle="modal" style="color: white;">'+
									val.namePrice +
									'</a></p></div>' +
									'<div class="_price_table_right"><span>' + val.price + ' тг</span></div>' +
								'</li>';
							tbl = tbl + tr;
						}
						
						if (val.isShowProd == 1) {
							divPrList = divPrList + '<div class="col-md-4 col-sm-6 col-xs-12 text-center">'+
								'<div class="_production_block wow flipInY" style="width: 345px; height: 408px">'+
								'<h4>'+val.name+'</h4>'+
								'<div style="width: 172px; height: 306px">'+
								'<img src="'+val.imgPath+'" style="height: 100%; width: 100%; object-fit: contain" alt="'+val.imgAlt+'">'+
								'</div>'+
								'<a href="#" data-target="#product-'+val.ID+'" data-toggle="modal">'+
								'<button>Подробное описание</button>'+
								'</a>'+
								'</div>'+
								'</div>';
						}
						divPrDescr = divPrDescr + '<div id="product-'+val.ID+'" class="modal fade">'+
							'<div class="modal-dialog">'+
							'<div class="modal-content text-center">'+
							'<div class="modal-header">'+
							'<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>'+
							'<h2 class="modal-title">'+val.name+'</h2>'+
							'</div>'+
							'<img src="'+val.imgPath+'" style="width: 172px; height: 306px; object-fit: contain; float: left;" alt="'+val.imgAlt+'">'+
							'<div class="modal-body text-product">'+
							''+val.descr+''+
							'</div>'+
							'</div>'+
							'</div>'+
							'</div>';
					});
				});
				$(tbl).appendTo("#ProductPrices");
				$(divPrList).appendTo("#productList");
				$(divPrDescr).appendTo("#productdescr");
			}
		});
	});
	
</script>
